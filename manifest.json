{
  "manifest_version": 2,
  
  "version": "0.1.0",
  
  "name": "Tag the Keep",
  "description": "Filters tagged notes in Google Keep, allowing for catogizing untagged notes",
  "homepage_url": "https://github.com/darthwalsh/TagTheKeep",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": ["activeTab"]
}